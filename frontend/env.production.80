# Production Environment Configuration (Port 80 - No Port in URL)
# This environment uses DNS naming for frontend identification
# Backend APIs are proxied through the same domain to avoid CORS issues

# Frontend Server Configuration
REACT_APP_PROD_SERVER_PORT=80
REACT_APP_PROD_SERVER_HOST=algodiscovery.prod

# API Base URLs - Using same domain to avoid CORS issues
# These will be proxied through Nginx to the actual backend services
REACT_APP_API_BASE_URL=http://algodiscovery.prod
REACT_APP_RECOMMENDATION_API_BASE_URL=http://algodiscovery.prod
REACT_APP_THEME_API_BASE_URL=http://algodiscovery.prod
REACT_APP_STRATEGY_API_BASE_URL=http://algodiscovery.prod
REACT_APP_DASHBOARD_API_BASE_URL=http://algodiscovery.prod
REACT_APP_INTRADAY_API_BASE_URL=http://algodiscovery.prod
REACT_APP_LONG_TERM_API_BASE_URL=http://algodiscovery.prod
REACT_APP_SWING_API_BASE_URL=http://algodiscovery.prod
REACT_APP_SHORT_TERM_API_BASE_URL=http://algodiscovery.prod
REACT_APP_UNIFIED_STRATEGY_API_BASE_URL=http://algodiscovery.prod
REACT_APP_VARIANTS_API_BASE_URL=http://algodiscovery.prod
REACT_APP_VALIDATION_API_BASE_URL=http://algodiscovery.prod
REACT_APP_MISC_API_BASE_URL=http://algodiscovery.prod
REACT_APP_FACTS_API_BASE_URL=http://algodiscovery.prod
REACT_APP_CANDIDATE_QUERY_REGISTRY_API_BASE_URL=http://algodiscovery.prod
REACT_APP_STOCK_CANDIDATE_POPULATOR_API_BASE_URL=http://algodiscovery.prod
REACT_APP_STOCK_MAPPING_API_BASE_URL=http://algodiscovery.prod
REACT_APP_ZERODHA_API_BASE_URL=http://algodiscovery.prod
REACT_APP_ZERODHA_TEST_API_BASE_URL=http://algodiscovery.prod
REACT_APP_CONTAINER_CONFIG_API_BASE_URL=http://algodiscovery.prod

# Request and Refresh Intervals (Production Optimized)
REACT_APP_REQUEST_INTERVAL=30000
REACT_APP_REFRESH_INTERVAL=60000
REACT_APP_BACKGROUND_REFRESH_INTERVAL=300000

# Environment Identification
REACT_APP_ENVIRONMENT=production
REACT_APP_ENV_NAME=Production (Port 80)
REACT_APP_ENV_DESCRIPTION=Production environment accessible via http://algodiscovery.prod (no port)

# Feature Flags
REACT_APP_ENABLE_DEBUG_LOGGING=false
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_ENABLE_ERROR_TRACKING=true

# Build Configuration
GENERATE_SOURCEMAP=false
REACT_APP_GENERATE_SOURCEMAP=false
